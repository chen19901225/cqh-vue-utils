{
	"name": "cqh-vue-utils",
	"displayName": "cqh-vue-utils",
	"description": "y",
	"version": "0.0.2",
	"publisher": "cqh",
	"repository": {
		"url": "https://github.com/chen19901225/cqh-vue-utils"
	},
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "cqh-vue-utils.export_var_to_return",
				"title": "export_var_to_return"
			}
		],
		"menus": {
            "editor/context": [
                {
                    "when": "!inOutput && editorTextFocus && resourceExtname  == .vue && editorHasSelection",
                    "command": "cqh-vue-utils.export_var_to_return",
                    "group": "cqh-vue-utils"
                }
            ]
        },
		"languages": [
            {
                "id": "vue",
                "aliases": [
                    "Vue",
                    "vue"
                ],
                "extensions": [
                    ".vue"
                ]
            }
        ],
		"snippets": [
            {
                "language": "vue",
                "path": "./snippets/snippets.json"
            }
        ]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.58.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2",
		"ts-loader": "^9.2.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	}
}
